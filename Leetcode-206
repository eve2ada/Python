'''
Reverse a singly linked list.
'''
# Definition for singly-linked list.
class ListNode:
    def __init__(self, x):
        self.val = x
        self.next = None

class Solution:
    # @param {ListNode} head
    # @return {ListNode}
    def reverseList(self, head):
        if not head:
            return None
        else:
            pre = head
            cur = pre.next
            pre.next = None
            while cur is not None:
                temp = cur.next
                cur.next = pre
                pre = cur
                cur = temp
            
            return pre
