def searchInsert(nums, target):
    """
    :type nums: List[int]
    :type target: int
    :rtype: int
    """
    if nums == []:
        return 0
    if target in nums:
        return nums.index(target)
    elif target < nums[0]:
        return 0
    elif target > nums[-1]:
        return len(nums)
    else:
        l, h = 0, len(nums)-1
        mid = (h-l)/2
        if target < nums[mid]:
            h = mid
        else:
            l = mid
        
        for index in range(l,h):
            if nums[index] < target and nums[index+1] > target:
                return index + 1
