def searchInsert(nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        if nums == []:
            return 0
        if target in nums:
            return nums.index(target)
        elif target < nums[0]:
            return 0
        elif target > nums[-1]:
            return len(nums)
        else:
            mid = len(nums)/2
            while target < nums[mid]:
                mid = mid/2
            for index in range(mid,mid*2):
                if nums[index] < target and nums[index + 1] > target:
                    return index + 1
            
            while target > nums[mid]:
                mid = mid + (len(nums)) /4
            for index in range(2*mid-len(nums),mid):
                if nums[index] < target and nums[index + 1] > target:
                    return index + 1
