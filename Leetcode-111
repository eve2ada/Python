'''
Given a binary tree, find its minimum depth.

The minimum depth is the number of nodes along the shortest path from the root node 
down to the nearest leaf node.
'''
# Definition for a binary tree node.
class TreeNode:
    def __init__(self, x):
        self.val = x
        self.left = None
        self.right = None

class Solution:
    # @param {TreeNode} root
    # @return {integer}
    def minDepth(self, root):
        if root is None:
            return 0
        
        if root.left is None and root.right is None:
            return 1
        else:
            count_min = 0
            count = 1
            stack = [root]
            while len(stack) > 0:
                temp = stack.pop(0)
#                count -= 1
                if [None, None] == [temp.left, temp.right]:
                    print 'here1'
                    print 'count_min = ' + str(count_min)
                    print 'count = ' + str(count) 
                    if count_min == 0:
                        count_min = count
                    elif count < count_min:
                        count_min = count
                if temp.right is not None:
                    print 'right'
                    count += 1
                    stack.insert(0,temp.right)
                if temp.left is not None:
                    print 'left'
                    count += 1
                    stack.insert(0, temp.left)

            return count_min
                
            
'''
#testcase 1
a = TreeNode(5)
a.val = 5
