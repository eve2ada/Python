def countPrimes(n):
    if n < 2:
        return 0
    else:
        newL = [0,0] + [1] * (n - 2)
        for i in range(2, int(n ** 0.5) + 1):
            if newL[i] == 1:
                j  = i * i
                while j < n:
                    newL[j] = 0
                    j += i
        return sum(newL)
