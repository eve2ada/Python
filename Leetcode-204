    def countPrimes(self, n):
        if n < 2:
            return 0
        else:
            i = 2
            newL = [0,0] + [1] * (n - 2)
            while i * i < n:
                if newL[i] == 1:
                    j  = i * i
                    while j < n:
                        newL[j] = 0
                        j += i
                i += 1
            return sum(newL)
