def countPrimes(n):
    from math import sqrt
    if n <= 2:
        return 0
    elif n == 3:
        return 1
    else:
        newL = [0,0] + [1] * (n - 2)
        for i in range(4, n + 1):
            for j in range(2, int(sqrt(i)) + 1):
                if newL[i - 1] == 1:
                    if i % j == 0:
                        newL[i - 1] = 0
        return sum(newL)
