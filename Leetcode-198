def rob(nums):
    n = len(nums)
    if nums == []:
        return 0
    elif n <= 2:
        return max(nums)
    else:
        return f(n - 1, nums)


def f(index, nums):
    temp = 0
    if index == 0:    
        temp = nums[0]
    elif index == 1:
        temp = max(nums[0], nums[1])
    else:
        temp = max(f(index - 1, nums), f(index - 2, nums) + nums[index])
    return temp
